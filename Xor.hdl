/** Exclusive-or
    out = a xor b

    Copyright (C) 2013 Eric Herman
    License: GNU General Public License v3 or any later version
*/
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=nandab);
    Nand(a=a, b=nandab, out=i1);
    Nand(a=nandab, b=b, out=i2);
    Nand(a=i1, b=i2, out=out);
}
