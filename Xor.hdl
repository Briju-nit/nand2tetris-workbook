/** Exclusive-or
    out = a xor b
*/
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=a, out=nota);
    Nand(a=b, b=b, out=notb);
    Nand(a=nota, b=notb, out=eitherone);

    Nand(a=a, b=b, out=nandab);
    Nand(a=nandab, b=nandab, out=both);

    Nand(a=both, b=both, out=notboth);

    Nand(a=eitherone, b=notboth, out=nandenb);
    Nand(a=nandenb, b=nandenb, out=out);
}
