/**
    Multiplexor
    out = (sel==1) ? b : a

    Copyright (C) 2013 Eric Herman
    License: GNU General Public License v3 or any later version
*/

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And(a=b, b=sel, out=bandsel);
    Not(in=sel, out=notsel);
    And(a=a, b=notsel, out=aandnotsel);
    Or(a=bandsel, b=aandnotsel, out=out);
}
